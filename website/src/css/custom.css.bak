/* Theme toggle button styling */
[class*="colorModeToggle_"] {
  color: #553630 !important;
}

[class*="colorModeToggle_"] svg {
  fill: #553630 !important;
}

html[data-theme='dark'] [class*="colorModeToggle_"] {
  color: #EB8788 !important;
}

html[data-theme='dark'] [class*="colorModeToggle_"] svg {
  fill: #EB8788 !important;
}

/* Hero title color for light mode */
[class*="hero__title"] {
  color: #553630 !important;
}

html[data-theme='dark'] [class*="hero__title"] {
  color: #FFF4E9 !important;
}

/* DocSearch styling for light mode */
.DocSearch {
  --docsearch-searchbox-background: #ffbcba34;
  --docsearch-searchbox-focus-background: #ffbcba55;
  --docsearch-key-gradient: linear-gradient(
    -26.5deg,
    #FDE5D8 0%,
    #FCE9D6 100%
  );
  --docsearch-key-shadow: 0 2px 2px #55363033;
  --docsearch-muted-color: #553630;
}

.DocSearch-Button {
  background-color: #FDE5D8 !important;
  padding: 0px 6px 0px 10px !important;
}

/* Extra selectors to ensure the style is applied */
[class*="docSearchButton_"], 
[class^="docSearchButton"],
div[class*="searchBox_"] button {
  background-color: #FDE5D8 !important;
  padding: 0px 6px 0px 10px !important;
}

.DocSearch-Button-Key {
  background: linear-gradient(to bottom, #654641 0%, #553630 100%) !important;
  border: 1px solid #44291e !important;
  border-radius: 6px !important;
  box-shadow: 0 3px 0 #44291e !important;
  color: #fff !important;
  transform: translateY(0);
  transition: transform 0.15s ease, box-shadow 0.15s ease;
  padding: 0 6px !important;
}

.DocSearch-Button:hover .DocSearch-Button-Key {
  transform: translateY(-1px);
  box-shadow: 0 4px 0 #44291e !important;
}

.DocSearch-Button:active .DocSearch-Button-Key {
  transform: translateY(2px);
  box-shadow: 0 1px 0 #44291e !important;
}

html[data-theme='dark'] .DocSearch {
  --docsearch-text-color: var(--ifm-font-color-base);
  --docsearch-muted-color: var(--ifm-color-secondary-darkest);
  --docsearch-container-background: rgba(46, 47, 40, 0.8);
  --docsearch-modal-background: var(--ifm-background-color);
  --docsearch-searchbox-background: var(--ifm-background-color);
  --docsearch-searchbox-focus-background: var(--ifm-color-black);
  --docsearch-hit-color: var(--ifm-font-color-base);
  --docsearch-hit-active-color: var(--ifm-color-white);
  --docsearch-hit-background: var(--ifm-color-emphasis-100);
  --docsearch-footer-background: var(--ifm-background-surface-color);
  --docsearch-key-gradient: linear-gradient(
    -26.5deg,
    #ba6663 0%,
    #944c4b 100%
  );
}

html[data-theme='dark'] .DocSearch-Button,
html[data-theme='dark'] [class*="DocSearch-Button"],
html[data-theme='dark'] [class*="docSearchButton_"],
html[data-theme='dark'] [class^="docSearchButton"],
html[data-theme='dark'] div[class*="searchBox_"] button {
  background-color: #3a2623 !important;
}

html[data-theme='dark'] .DocSearch-Button-Key {
  background: linear-gradient(to bottom, #f9a7a8 0%, #EB8788 100%) !important;
  border: 1px solid #d67573 !important;
  border-radius: 6px !important;
  box-shadow: 0 3px 0 #d67573 !important;
  color: #2A1A17 !important;
  text-shadow: 0 1px 0 #ffbcba22 !important;
  transform: translateY(0);
  transition: transform 0.15s ease, box-shadow 0.15s ease;
}

html[data-theme='dark'] .DocSearch-Button:hover .DocSearch-Button-Key {
  transform: translateY(-1px);
  box-shadow: 0 4px 0 #d67573 !important;
}

html[data-theme='dark'] .DocSearch-Button:active .DocSearch-Button-Key {
  transform: translateY(2px);
  box-shadow: 0 1px 0 #d67573 !important;
}

.markdown > table {
  border: none;
  min-width: 160px;
}

.markdown > table td {
  min-width: 160px;
}

/* Custom announcement bar styling */
div[class^="announcementBar"] {
  background-color: #EB8788 !important;
  /* padding: 16px 0 !important; */
  height: 36px;
}

[class*="announcementBar_"] {
  border-bottom: 0px !important;
}

div[class^="announcementBarContent"] {
  color: white !important;
  font-weight: 400;
}

/* Fix announcement bar close button alignment */
div[class^="announcementBarClose"] {
  display: flex;
  align-items: center;
  justify-content: center;
  top: 50%;
  transform: translateY(-50%);
  padding: 0;
  margin-right: 12px;
}

/* Reduce paragraph line height */
p {
  line-height: 1.4;
}

/* Reduce code block font size */
pre, code {
  font-size: 12px !important;
}

/* Navbar logo styling */
.navbar__logo {
  height: 32px;
}

.navbar__logo img {
  height: 100%;
  width: auto;
}

/* Navbar brand spacing */
.navbar__brand {
  margin-right: 0rem;
}

/* Navbar links styling - reduced font size */
.navbar__link {
  font-size: 14px; /* Reduced font size for navbar links */
}

/* Add navbar dark mode box-shadow */
html[data-theme="dark"] .navbar {
  box-shadow: 0px 1px #444950;
}

/* Keep the navbar brand/title at normal size */
.navbar__title {
  font-size: 18px; /* Keep the original size or adjust as needed */
  display: none !important; /* Hide the navbar title with !important to override other styles */
  width: 0;
  height: 0;
  overflow: hidden;
  opacity: 0;
  visibility: hidden;
}

/* Override themed component images */
div[class*="themedImage"],
div[class*="themedComponent"] {
  height: auto;
  width: auto;
}

div[class*="themedImage"] img,
div[class*="themedComponent"] img,
div[class*="themedComponent--light"] img,
div[class*="themedComponent--dark"] img {
  height: 100%;
  width: auto;
  max-height: 300px;
}

/* 3D Extruded Button Styles */
.button.button--primary {
  background-color: #FFBCBA !important;
  color: #333 !important;
  border: none !important;
  border-radius: 6px !important;
  position: relative;
  box-shadow: 0 6px 0 #E8908E !important;
  transform: translateY(0);
  transition: transform 0.2s ease, box-shadow 0.2s ease, background-color 0.3s ease;
  font-weight: 600;
}

.button.button--primary:hover {
  background-color: #FFA7A5 !important;
  transform: translateY(-2px);
  box-shadow: 0 8px 0 #D67573 !important;
}

.button.button--primary:active {
  transform: translateY(4px);
  box-shadow: 0 2px 0 #D67573 !important;
  background-color: #E89896 !important;
  transition: all 0.05s ease-in;
}

.button.button--outline.button--primary {
  color: #333 !important;
  border: 2px solid #FFBCBA !important;
  background-color: #ffbcba9e !important;
  box-shadow: 0 6px 0 #E8908E !important;
  transition: background-color 0.3s ease, border-color 0.3s ease, transform 0.2s ease, box-shadow 0.2s ease;
}

.button.button--outline.button--primary:hover {
  background-color: #FFA7A5 !important;
  border-color: #FFA7A5 !important;
  transform: translateY(-2px);
  box-shadow: 0 8px 0 #D67573 !important;
}

.button.button--outline.button--primary:active {
  transform: translateY(4px);
  box-shadow: 0 2px 0 #D67573 !important;
  background-color: #E89896 !important;
  transition: all 0.05s ease-in;
}

/* Dark mode button styling fixes */
html[data-theme="dark"] .button.button--primary,
html[data-theme="dark"] .button.button--outline.button--primary {
  color: #fff !important;
  background-color: #ba6663 !important;
}

/* Style paragraph links to be dark pink and semibold */
p a:not(.button) {
  color: #d32f2f !important; /* Darker red */
  font-weight: 600;
}

html[data-theme="dark"] p a:not(.button) {
  color: #ff5252 !important; /* Brighter red for dark mode */
}

/* Target code blocks with increased padding */
.padding--none, div[class*=codeBlockLines_] pre[class*=prism-code], div[class*=theme-code-block]>div>pre, pre[class*=codeBlock_] {
    padding: 1rem !important;
}

/* Homepage-only code blocks with max height */
.homepage [class*="codeBlockLines_"] {
  max-height: 350px;
  overflow-y: scroll;
  overflow-x: hidden !important;
}

/* Light mode code block styling */
html[data-theme='light'] div[class*="codeBlockLines_"],
html[data-theme='light'] pre[class*="prism-code"],
html[data-theme='light'] .codeBlockLines_node_modules-\@docusaurus-theme-classic-lib-theme-CodeBlock-Content-styles-module {
  background-color: #FDE5D8 !important;
  color: var(--ifm-color-gray-900) !important;
}

/* Light mode code block container styling */
html[data-theme='light'] div[class*="theme-code-block"],
html[data-theme='light'] div[class*="codeBlockContainer"] {
  background-color: #FDE5D8 !important;
}

/* Move code block button group to accommodate scrollbar */
.buttonGroup_node_modules-\@docusaurus-theme-classic-lib-theme-CodeBlock-Content-with,
div[class*="buttonGroup_"] {
  background-color: #FDE5D8 !important;
  padding-bottom: 0.5rem !important;
}

/* Dark mode code block styling */
html[data-theme='dark'] div[class*="codeBlockLines_"],
html[data-theme='dark'] pre[class*="prism-code"],
html[data-theme='dark'] .codeBlockLines_node_modules-\@docusaurus-theme-classic-lib-theme-CodeBlock-Content-styles-module {
  background-color: #3a2623 !important;
  color: #ffffff !important;
}

/* Additional dark mode code selector to ensure proper styling */
html[data-theme='dark'] [class*="theme-code-block"] {
  background-color: #242521 !important;
  color: #ffffff !important;
}

/* Custom Tab Item styling - reduce vertical padding */
div[class*="tabItem_"] {
  padding-top: 0.5rem !important;
  padding-bottom: 0.5rem !important;
}

/* Make sure enough space for the code blocks inside tabs */
div[class*="tabItem_"] > div {
  margin-top: 0.25rem;
}

/* Custom container padding */
/* Default padding for screens below 1675px */
.container.padding-top--md {
  padding-top: var(--ifm-spacing-vertical) !important; /* Default 1rem padding */
}

/* Increased padding for screens larger than 1675px */
@media (min-width: 1675px) {
  .container.padding-top--md {
    padding-top: calc(var(--ifm-spacing-vertical) * 3) !important;
  }
}

.container.padding-bottom--md {
  padding-bottom: calc(var(--ifm-spacing-vertical) * 3) !important; /* Convert md to lg padding */
}

/* Custom margin adjustment */
.margin-vert--xl {
  margin-bottom: 3rem !important;
  margin-top: 3rem !important;
}

/* Override for cookbooks page */
[class*="CookbookPage"] .margin-vert--xl {
  margin-top: 2rem !important;
  margin-bottom: 2rem !important;
}

/* Full-width container with no padding */
.container-fluid {
  width: 100%;
  margin-right: auto;
  margin-left: auto;
}

/* Tabbed section styling */
.tabbedSection {
  background-color: #f9f9f9;
  padding: 60px 0;
  margin: 40px 0;
}

html[data-theme="dark"] .tabbedSection {
  background-color: #2a2b25;
}

.tabbedSectionImg {
  max-height: 250px;
  width: auto;
  margin-right: 20px;
  margin-top: -30px;
}

