/**
 * Any CSS included here will be global. The classic template
 * bundles Infima by default. Infima is a CSS framework designed to
 * work well for content-centric websites.
 */

/* Import IBM Plex Sans from Google Fonts */
@import url('https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:wght@300;400;500;600;700&display=swap');

:root {
  --ifm-color-primary: #d32f2f;
  --ifm-color-primary-dark: #be2a2a;
  --ifm-color-primary-darker: #b32727;
  --ifm-color-primary-darkest: #942020;
  --ifm-color-primary-light: #d84545;
  --ifm-color-primary-lighter: #db5151;
  --ifm-color-primary-lightest: #e27676;

  --ifm-color-danger: #ff6188;
  --ifm-color-danger-dark: #ff3e6d;
  --ifm-color-danger-darker: #ff2c60;
  --ifm-color-danger-darkest: #f6003d;
  --ifm-color-danger-light: #ff84a3;
  --ifm-color-danger-lighter: #ff96b0;
  --ifm-color-danger-lightest: #ffcbd8;

  --ifm-color-success: #a9dc76;
  --ifm-color-success-dark: #98d55b;
  --ifm-color-success-darker: #90d24e;
  --ifm-color-success-darkest: #76bc30;
  --ifm-color-success-light: #bae391;
  --ifm-color-success-lighter: #c2e69e;
  --ifm-color-success-lightest: #dcf1c7;

  --ifm-color-info: #78dce8;
  --ifm-color-info-dark: #5ad4e3;
  --ifm-color-info-darker: #4bd0e0;
  --ifm-color-info-darkest: #24c0d3;
  --ifm-color-info-light: #96e4ed;
  --ifm-color-info-lighter: #a5e8f0;
  --ifm-color-info-lightest: #d2f3f7;

  --ifm-color-warning: #ffd866;
  --ifm-color-warning-dark: #ffcf42;
  --ifm-color-warning-darker: #ffca30;
  --ifm-color-warning-darkest: #faba00;
  --ifm-color-warning-light: #ffe18a;
  --ifm-color-warning-lighter: #ffe69c;
  --ifm-color-warning-lightest: #fff3d1;

  --ifm-navbar-background-color: #FFF4E9;
  --ifm-background-surface-color: #FFF4E9;
  --ifm-background-surface-color-secondary: #FFEFE0;
  --ifm-background-color: #FFF4E9;

  --ifm-heading-color: #000000;
  --ifm-hero-text-color: #000000;
  --ifm-hero-background-color: #ffbcba34;
  --ifm-blockquote-color: #3c3c3c;
  --ifm-alert-color: #000000;

  --ifm-code-font-size: 95%;
  
  --ifm-code-background: #FCE9D6;
  
  /* Set IBM Plex Sans as the default font family */
  --ifm-font-family-base: 'IBM Plex Sans', system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
}

/* Apply the font to the body */
body {
  font-family: var(--ifm-font-family-base);
}

/* Set h1 size to 2rem */
h1 {
  font-size: 2rem;
}

/* Set all headings in light mode to #553630 except in loveSection */
h1:not([class*="loveSection_"] h1), 
h2:not([class*="loveSection_"] h2), 
h3:not([class*="loveSection_"] h3), 
h4:not([class*="loveSection_"] h4), 
h5:not([class*="loveSection_"] h5), 
h6:not([class*="loveSection_"] h6) {
  color: #553630;
}

/* Sponsored by text styling */
#sponsored-by {
  font-size: 18px;
  font-weight: 500;
}

.footer.footer--dark {
  --ifm-footer-background-color: var(--ifm-background-color);
  --ifm-footer-color: var(--ifm-font-color-base);
  --ifm-footer-link-color: var(--ifm-color-primary);
  --ifm-footer-title-color: var(--ifm-heading-color);
  padding: 6rem 0;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  position: relative;
  text-align: center;
}

.footer__inner {
  max-width: 900px;
  margin: 0 auto;
  padding: 0 1rem;
  text-align: center;
}

html[data-theme="dark"] .footer.footer--dark {
  border-top: 1px solid rgba(255, 255, 255, 0.1);
}

.footer__copyright {
  text-align: center;
  padding-top: 2rem;
  font-size: 0.8rem;
  opacity: 0.7;
  max-width: 900px;
  margin: 0 auto;
  padding-left: 2rem;
  padding-right: 2rem;
}

/* Override container padding in footer */
.footer .container,
.footer .container-fluid {
  padding: 0;
  text-align: center;
}

/* Remove padding from footer columns while preserving other .col styling */
.footer .col {
  padding: 0 1rem !important;
  text-align: center;
}

.footer__title {
  font-weight: 600;
  margin-bottom: 1rem;
  text-align: center;
}

.footer__link-item {
  line-height: 1.2;
  transition: color 0.2s;
  display: inline-block;
}

/* Remove external link icons from footer links */
.footer__link-item svg {
  display: none;
}

.footer__links {
  text-align: center;
  margin-bottom: 1rem;
}

.footer__link-item:hover {
  color: var(--ifm-color-primary-dark);
  text-decoration: none;
}

html[data-theme="dark"] {
  --ifm-color-primary: #ff5252;
  --ifm-color-primary-dark: #1ccaff;
  --ifm-color-primary-darker: #0cc6ff;
  --ifm-color-primary-darkest: #00a8dc;
  --ifm-color-primary-light: #5ad8ff;
  --ifm-color-primary-lighter: #6adcff;
  --ifm-color-primary-lightest: #99e7ff;

  --ifm-background-color: #2A1A17;
  --ifm-background-surface-color: #272822;
  --ifm-background-surface-color-secondary: #2a2b25;
  --ifm-navbar-background-color: #2A1A17;

  --ifm-code-background: #2d2e27;
  --ifm-code-color: #ffffff;

  background-color: #2A1A17 !important;
  color: #fff4e9 !important;

  --ifm-heading-color: #FFF4E9;
  --ifm-hero-text-color: #FFF4E9;
  --ifm-hero-background-color: #ffbcba12;
  --ifm-color-secondary: #FFF4E9;
  --ifm-blockquote-color: #FFF4E9;
  --ifm-font-color-base: #FFF4E9;
}

/* Light mode inline code styling */
code {
  background-color: #FDE5D8 !important;
  color: var(--ifm-code-color) !important;
  border: none !important;
  padding: 3px 7px !important;
}

/* Dark mode inline code styling */
html[data-theme='dark'] code {
  background-color: #3a2623 !important;
  color: #ffffff !important;
}

a > code {
  color: var(--ifm-link-color) !important;
}

a.contents__link > code {
  color: var(--ifm-code-color);
}

.menu--responsive > button {
  background-color: var(--ifm-color-primary);
  border-color: var(--ifm-color-primary);
}

.header-icon-link {
  padding: var(--ifm-navbar-item-padding-vertical) calc(var(--ifm-navbar-item-padding-horizontal) * 0.8);
}

.header-icon-link:hover {
  opacity: 0.6;
}

